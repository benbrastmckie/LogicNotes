\section*{\sc First-Order Logic: Semantics}

\begin{enumerate}[leftmargin=1.2in] %,label=(\arabic*)]%,label=\roman*]
	\item[\bf Domain:] Let the \textit{domain} $\mathcal{D}$ be a set of objects.% and $\mathcal{D}^n=\set{\tuple{d_1,\dots,d_n}:d_i\in\mathcal{D}}$.
	\item[\bf Cartesian Domain:] Let $\mathcal{D}^n$ be the set of all ordered tuples $\tuple{d_1,\dots,d_n}$ where each $d_i$ is an object in the domain $\mathcal{D}$, i.e., $\mathcal{D}^n=\set{\tuple{d_1,\dots,d_n}:d_i\in\mathcal{D}~\text{for}~1\leq i\leq n}$.
	\item[\bf Interpretation:] Let $\FImodels$ be an \textit{interpretation} of $\FI$ over $\mathcal{D}$ just in case: (i) $\FImodels(p_i^n)\subseteq\mathcal{D}^n$ for every $i\geq1$ and $n\geq0$; and (ii) $\FImodels(c_i)\in\mathcal{D}$ for every $i\geq1$.
	      %\item[\bf Extension:] The \textit{extension} of an $n$-place predicate `$p_i^n$' of $\FI$ on an interpretation $\FImodels$ over a domain $\mathcal{D}$ is the set of ordered tuples $\FImodels(p_i^n)\subseteq\mathcal{D}^n$.
	\item[\bf Assignment:] An \textit{assignment} $\underline{a}$ is a function from the variables in $\FI$ to the members of $\mathcal{D}$ such that $\underline{a}(x_i)$ is a member of the domain $\mathcal{D}$ for every $i\geq1$.
	\item[\bf Denotation:] Let $I(t)=
		      \begin{cases}
			      \FImodels(t)     & \text{if}~~ t=c_i ~~\text{for any}~~ i\geq1 \\
			      \underline{a}(t) & \text{if}~~ t=x_i ~~\text{for any}~~ i\geq1
		      \end{cases}$
	\item[\bf Variant:] The function $\underline{a}[d/x]$ is an \textit{$x$-variant} of the assignment $\underline{a}$ just in case $\underline{a}[d/x]$ differs from $\underline{a}$ at most by setting $\underline{a}[d/x](x)=d$.
	\item[\bf Model:] A \textit{model} of $\FI$ is any ordered pair $\M=\tuple{\mathcal{D},\FImodels}$, where $\mathcal{D}$ is a domain of individuals, and $\FImodels$ an interpretation over $\mathcal{D}$.
	\item[\bf Semantics:] Given a model $\M$ of $\FI$, and assignment $\underline{a}$, we may recursively define $\M,\underline{a}\vDash A$ for all wfss $A$ of $\FI$ as follows:
	      \begin{small}
		      \begin{itemize}[leftmargin=.36in]
			      \item[$(p_i)$] $\M,\underline{a}\vDash  p_i^n(t_1,\dots,t_n)$ \textit{iff} $\tuple{I(t_1),\dots,I(t_n)}\in \FImodels(p_i^n)$.
			      \item[$(\hspace{1.5pt}\exists\hspace{1.5pt})$] $\M,\underline{a}\vDash  \exists x_iA$ \textit{iff} $\M,\underline{a}[d/x_i]\vDash A$, for some $d\in\mathcal{D}$.
			      \item[$(\neg)$] $\M,\underline{a}\vDash  \neg A$ \textit{iff} $\M,\underline{a}\nvDash A$.
			      \item[$(\vee)$] $\M,\underline{a}\vDash  A\vee B$ \textit{iff} $\M,\underline{a}\vDash  A$ or $\M,\underline{a}\vDash  B$.
			            %\item[$(\wedge)$] $\PLmodel\vDash A\wedge B$ \textit{iff} $\PLmodel\vDash A$ and $\PLmodel\vDash B$. 
			            %\item[$(\shortrightarrow)$] $\PLmodel\vDash A\rightarrow B$ \textit{iff} $\nvDash_{\PLmodel} A$ or $\PLmodel\vDash B$.
			            %\item[$(\leftrightarrow)$] $\PLmodel\vDash A\leftrightarrow B$ \textit{iff} $\PLmodel\vDash A$ and $\PLmodel\vDash B$, or $\nvDash_{\PLmodel} A$ and $\nvDash_{\PLmodel} B$.
		      \end{itemize}
	      \end{small}
	      It is important that in the case where $n=0$, we adopt the convention that $\FImodels(p_i^0)=\set{\varnothing}$ indicates truth, and $\FImodels(p_i^0)=\varnothing$ indicates falsity.
\end{enumerate}



\section*{\sc First-Order Logic: Metalogic}%CONTINUE

\begin{enumerate}[leftmargin=1.2in] %,label=(\arabic*)]%,label=\roman*]
	\item[\bf Truth on a Model:] $\M\vDash_1 A$ \textit{iff} $\M,\underline{a}\vDash A$ for all variable assignments $\underline{a}$.
	\item[\bf Logical Consequence:] $\MetaG\vDash_1 A$ \textit{iff} for all models $\M$, if $\M\vDash G$ for all $G\in\MetaG$, then $\M\vDash A$.
	\item[\bf Logical Equivalence:] $A\equiv_1 B$ \textit{iff} $A\vDash_1 B$ and $B\vDash_1 A$.
	\item[\bf Logical Truth:] A wfs $A$ of $\FI$ is \textit{valid} (or a logical truth) just in case $\vDash_1 A$.
	\item[\bf First-Order Logic:] The first-order formal system of natural deduction $\mathcal{F}_1^+=\tuple{\FI,\mathcal{G}_1^+,\mathcal{A}_1^+,\mathcal{R}_1^+}$ is sound and complete, where $\mathcal{A}_1^+=\varnothing$.
\end{enumerate}





\section*{\it Problem Set: First-Order Logic\footnote{I have adapted some of the following problems from \citet{Carr2013}. See also \citet{Halbach2010}.}}

\begin{enumerate}[leftmargin=1.2in]
	\item[\bf Semantics:] Provide a semantics for the wfss of $\FI$.
	\item[\bf Regimentation:] Regiment the following arguments into $\FI$.
	      \begin{enumerate}[label=(\arabic*)]\small
		      %\item $\forall xPx\vdash \forall yPy$.
		      \item Everything that is beautiful is beautiful.
		      \item Every philosopher is happy. So if everything is a philosopher, everything is happy.
		      \item Everything is a philosopher and everything is happy. It follows that everything is a happy philosopher.
		      \item Something is such that it is happy if Ella is a philosopher. So if Ella is a philosopher, then something is happy.
		      \item There is a beautiful country. And so something is beautiful and something is a country.
		      \item Nothing is ugly, and so everything is not ugly.
		      \item Something is not right. It follows that not everything is right.
		      \item Not everything is free. And so something is not free.
		      \item Everything is not free. It follows that nothing is free.
		      \item Every philosopher is wise, and everything wise is happy. Thus, every philosopher is happy.
		      \item Every philosopher is happy. There is a wise philosopher. And something is wise and happy.
		      \item Everything loves everything. Thus, everything loves itself.
		      \item Something loves itself. And so something loves something.
		      \item Nothing loves something which returns its loves.
		            %\item Some of the people can be all right part of the time, but all the people can't be all right all the time.
	      \end{enumerate}
	\item[\bf Deduction:] Use the natural deduction rules $\mathcal{R}_1^+$ to prove that the conclusion of each of the regimented arguments above follows from its premises.
	      %\begin{enumerate}[label=(\arabic*)]
	      %\item $\forall xPx\vdash \forall yPy$.
	      %\item $\vdash \forall x(Px\rightarrow Px)$.
	      %\item $\forall x(Pa\rightarrow Qx)\vdash Pa\rightarrow\forall xQx$.
	      %\item $\forall x(Px\rightarrow Qx)\vdash \forall Px\rightarrow\forall xQx$.
	      %\item $\forall xPx\wedge\forall xQx\vdash \forall y(Py\wedge Qy)$.
	      %\item $\exists x(Pa\rightarrow Qx)\vdash Pa\rightarrow\exists xQx$.
	      %\item $\exists x(Px\wedge Qx)\vdash \exists yPy\wedge \exists yQy$.
	      %\item $\neg\exists xPx\vdash \forall y\neg Py$.
	      %\item $\exists x\neg Px\vdash \neg\forall yPy$.
	      %\item $\neg\forall xPx\vdash\exists y\neg Py$.
	      %\item $\forall x\neg Px\vdash \neg\exists yPy$.
	      %\item $\forall x(Qx\rightarrow Rx), \forall x(Px\rightarrow Qx)\vdash\forall x(Px\rightarrow Rx)$.
	      %\item $\forall x(Qx\rightarrow Rx),\exists x(Px\wedge Qx)\vdash\exists x(Px\wedge Rx)$.
	      %\item $\forall x\forall yRxy\vdash\forall xRxx$.
	      %\item $\exists xRxx\vdash\exists x\exists yRxy$.
	      %\item $\neg\exists x\exists y(Rxy\wedge Ryx)\vdash\neg\exists xRxx$.
	      %\end{enumerate}
	\item[\bf Metalogic:] Prove that every theorem of $\mathcal{F}^+$ is also a theorem of $\mathcal{F}^+_1$.
	\item[\bf Bonus:] Regiment the following into $\FI$:
	      \begin{enumerate}[label=(\arabic*)]\small
		      \item Everybody loves somebody.
		      \item Everybody everybody loves loves somebody.
		      \item Everybody everybody everybody loves loves loves somebody.
		            %\item Everybody everybody everybody everybody loves loves loves loves somebody.
		      \item You can fool all the people some of the time, and some of the people all the time, but you cannot fool all the people all the time.
	      \end{enumerate}
\end{enumerate}







%\section*{\sc Quantified Modal Logic: Syntax}

%\section*{\sc Quantified Modal Logic: Axiomatic Systems}

%\section*{\sc Quantified Modal Logic: Semantics}


%----------------------------------------------------------------------------------------
%   TM LOGIC SYSTEM (BIMODAL LOGIC)
%   Added: 2025-12-02
%   Source: possible_worlds.tex lines 1018-2684
%----------------------------------------------------------------------------------------

\section*{\sc Bimodal Logic: Tense and Modality}
  \label{sec:TenseModality}

Natural language frequently expresses relationships between temporal and modal concepts. Consider these intuitions:

\begin{enumerate}[leftmargin=1.2in,itemsep=4pt]
  \item[\bf Perpetuity:] ``If something is necessary, then it has always been and will always be true.'' Formally: if $\Box \metaA$, then $\metaA$ holds at all times (past, present, and future).
  \item[\bf Possibility:] ``If something is sometimes true, then it is possibly true.'' Formally: if $\metaA$ holds at some time, then $\Diamond \metaA$.
  \item[\bf Interaction:] ``Necessary truths persist through time.'' If $\Box \metaA$ at time $t$, then $\Box \metaA$ at all future times.
\end{enumerate}

These principles cannot be captured by modal logic alone (which lacks temporal operators) nor by tense logic alone (which lacks modal operators). We require a \textit{bimodal logic} combining both dimensions. The \textbf{TM system} (Logic of Tense and Modality) provides such a framework, integrating S5 modal logic with linear tense logic through specific interaction axioms.

\section*{\sc TM Logic: Syntax}

\begin{enumerate}[leftmargin=1.2in]
  \item[\bf Language:] The \textit{bimodal language} $\BL$ extends propositional logic with modal and temporal operators:
    \[
      \BL = \tuple{\SL, \bot, \rightarrow, \Box, \mathsf{Past}, \mathsf{Future}}
    \]
    where $\SL = \set{p_i \mid i \in \N}$ is the set of sentence letters.

  \item[\bf Well-Formed Sentences:] The set of well-formed sentences $\wfs{\BL}$ is defined inductively:
    \[
      \metaA ::= p_i \mid \bot \mid \metaA \rightarrow \metaA \mid \Box \metaA \mid \Past \metaA \mid \Future \metaA
    \]
    where $p_i \in \SL$.

  \item[\bf Abbreviations:] We define the following derived operators:
    \begin{enumerate}[leftmargin=.5in,labelsep=.15in,itemsep=.075in]
      \item \textbf{Possibility}: $\Diamond \metaA := \neg\Box\neg\metaA$
      \item \textbf{Past possibility}: $\past \metaA := \neg\Past\neg\metaA$
      \item \textbf{Future possibility}: $\future \metaA := \neg\Future\neg\metaA$
      \item \textbf{Always}: $\always \metaA := \Past \metaA \wedge \metaA \wedge \Future \metaA$
      \item \textbf{Sometimes}: $\sometimes \metaA := \past \metaA \vee \metaA \vee \future \metaA$
      \item \textbf{Standard}: $\neg\metaA$, $\metaA \vee \metaB$, $\metaA \wedge \metaB$, $\metaA \leftrightarrow \metaB$ as usual.
    \end{enumerate}

  \item[\bf Interpretation:] The temporal operators have the following informal meanings:
    \begin{enumerate}[leftmargin=.5in,labelsep=.15in,itemsep=.075in]
      \item $\Past \metaA$: ``$\metaA$ has always been true'' (at all past times)
      \item $\past \metaA$: ``$\metaA$ was true at some past time''
      \item $\Future \metaA$: ``$\metaA$ will always be true'' (at all future times)
      \item $\future \metaA$: ``$\metaA$ will be true at some future time''
      \item $\always \metaA$: ``$\metaA$ is true at all times'' (past, present, future)
      \item $\sometimes \metaA$: ``$\metaA$ is true at some time'' (past, present, or future)
    \end{enumerate}

  \item[\bf Scope:] As in modal logic, $\Box$, $\Past$, and $\Future$ bind more tightly than connectives. Thus:
    \begin{enumerate}[leftmargin=.5in,labelsep=.15in,itemsep=.075in]
      \item $\Box \metaA \rightarrow \metaB$ means $(\Box \metaA) \rightarrow \metaB$
      \item $\Future \metaA \vee \metaB$ means $(\Future \metaA) \vee \metaB$
      \item $\always \metaA \wedge \metaB$ means $(\always \metaA) \wedge \metaB$
    \end{enumerate}
\end{enumerate}

\section*{\sc TM Logic: Axiomatic System}

\begin{enumerate}[leftmargin=1.2in]
  \item[\bf TM System:] The \textit{TM proof system} $\TMSystem$ is the smallest set of $\BL$ formulas containing all instances of the axiom schemata below and closed under the inference rules. A \textit{TM-proof} of $\metaA$ from premises $\MetaG$ is a finite sequence of formulas $\metaA_1, \ldots, \metaA_n$ where $\metaA_n = \metaA$ and each $\metaA_i$ is either a member of $\MetaG$, an axiom instance, or follows from earlier formulas by a rule. We write $\MetaG \TMproves \metaA$ when such a proof exists.

  \item[\bf Propositional Axioms:] All tautologies of propositional logic are TM axioms. In particular:
    \begin{enumerate}[leftmargin=.5in,labelsep=.15in,itemsep=.075in]
      \item $\metaA \rightarrow (\metaB \rightarrow \metaA)$
      \item $(\metaA \rightarrow (\metaB \rightarrow \metaC)) \rightarrow ((\metaA \rightarrow \metaB) \rightarrow (\metaA \rightarrow \metaC))$
      \item $(\neg\metaA \rightarrow \neg\metaB) \rightarrow (\metaB \rightarrow \metaA)$
    \end{enumerate}

  \item[\bf Modal Axioms and Rules:] The modal fragment of TM is S5. We have:
    \begin{enumerate}[leftmargin=.5in,labelsep=.15in,itemsep=.075in]
      \begin{multicols}{2}
        \item[\bf MP:] If $\MetaG \TMproves \metaA$ and $\MetaG \TMproves \metaA \rightarrow \metaB$, then $\MetaG \TMproves \metaB$ (modus ponens)
        \item[\bf MK:] If $\MetaG \TMproves \metaA$, then $\Box\MetaG \TMproves \Box\metaA$ (necessitation)
        \item[\bf MT:] $\Box\metaA \rightarrow \metaA$ (reflexivity)
        \item[\bf M4:] $\Box\metaA \rightarrow \Box\Box\metaA$ (transitivity)
        \item[\bf MB:] $\metaA \rightarrow \Box\Diamond\metaA$ (symmetry)
        \item[\bf MF:] $\Box\metaA \rightarrow \Box\Future\metaA$ (modal-future interaction)
      \end{multicols}
    \end{enumerate}
    where $\Box\MetaG = \set{\Box\metaC \mid \metaC \in \MetaG}$. The axioms MT, M4, and MB ensure the modal operator $\Box$ has the properties of S5: reflexivity, transitivity, and symmetry (equivalently, reflexivity and Euclideanness).

  \item[\bf Temporal Axioms and Rules:] The temporal operators obey linear, transitive temporal logic:
    \begin{enumerate}[leftmargin=.5in,labelsep=.15in,itemsep=.075in]
      \begin{multicols}{2}
        \item[\bf TD:] If $\TMproves \metaA$, then $\TMproves \metaA[\Past/\Future]$ (temporal duality)
        \item[\bf TK:] If $\MetaG \TMproves \metaA$, then $\Future\MetaG \TMproves \Future\metaA$ (future distribution)
        \item[\bf TL:] $\always\metaA \rightarrow \Future\Past\metaA$ (linearity)
        \item[\bf T4:] $\Future\metaA \rightarrow \Future\Future\metaA$ (temporal transitivity)
        \item[\bf TA:] $\metaA \rightarrow \Future\past\metaA$ (temporal accessibility)
        \item[\bf TF:] $\Box\metaA \rightarrow \Future\Box\metaA$ (modal-future persistence)
      \end{multicols}
    \end{enumerate}
    where $\Future\MetaG = \set{\Future\metaC \mid \metaC \in \MetaG}$, and $\metaA[\Past/\Future]$ is the result of simultaneously swapping all occurrences of $\Past$ with $\Future$ and vice versa. Axiom TD ensures the past and future are symmetric; TK ensures future distributes over derivability; TL ensures time is linear; T4 ensures future is transitive; TA ensures the present is accessible from the future.

  \item[\bf Perpetuity Principles:] The interaction between modal and temporal operators yields important perpetuity theorems. The following are all TM-derivable:
    \begin{enumerate}[leftmargin=.5in,labelsep=.15in,itemsep=.075in]
      \begin{multicols}{2}
        \item[\bf P1:] $\Box\metaA \rightarrow \always\metaA$ (necessity implies always)
        \item[\bf P2:] $\sometimes\metaA \rightarrow \Diamond\metaA$ (sometimes implies possibility)
        \item[\bf P3:] $\Box\metaA \rightarrow \Box\always\metaA$ (necessary always)
        \item[\bf P4:] $\Diamond\sometimes\metaA \rightarrow \Diamond\metaA$ (possibly sometimes)
        \item[\bf P5:] $\Diamond\sometimes\metaA \rightarrow \always\Diamond\metaA$ (persistent possibility)
        \item[\bf P6:] $\sometimes\Box\metaA \rightarrow \Box\always\metaA$ (sometimes necessary)
      \end{multicols}
    \end{enumerate}
    where $\always\metaA := \Past\metaA \wedge \metaA \wedge \Future\metaA$ and $\sometimes\metaA := \past\metaA \vee \metaA \vee \future\metaA$.
\end{enumerate}

\vspace{0.2in}
\noindent\textbf{Proof of P1} ($\Box\metaA \rightarrow \always\metaA$):

\begin{hilbert}
  \from{Assumption}{(\Box\metaA \rightarrow \always\metaA) \rightarrow ((\Box\metaA \rightarrow \always\metaA) \rightarrow (\Box\metaA \rightarrow \always\metaA))}
  \from{\aref{MF}}{\Box\metaA \rightarrow \Box\Future\metaA}
  \from{\aref{MT}}{\Box\Future\metaA \rightarrow \Future\metaA}
  \from{\aref{MP} (2,3)}{\Box\metaA \rightarrow \Future\metaA}
  \from{\aref{MF}, \aref{TD}}{\Box\metaA \rightarrow \Box\Past\metaA}
  \from{\aref{MT}}{\Box\Past\metaA \rightarrow \Past\metaA}
  \from{\aref{MP} (5,6)}{\Box\metaA \rightarrow \Past\metaA}
  \from{\aref{MT}}{\Box\metaA \rightarrow \metaA}
  \from{PL (4,7,8)}{\Box\metaA \rightarrow (\Past\metaA \wedge \metaA \wedge \Future\metaA)}
  \from{Abbreviation}{\Box\metaA \rightarrow \always\metaA}
\end{hilbert}

\vspace{0.1in}
\noindent\textbf{Proof of P2} ($\sometimes\metaA \rightarrow \Diamond\metaA$): By TD (temporal duality), P2 is the temporal dual of P1. Alternatively, note that $\sometimes\metaA \equiv \neg\always\neg\metaA$ and $\Diamond\metaA \equiv \neg\Box\neg\metaA$ by definition. Then P2 follows from P1 by contraposition:
\begin{align*}
  \Box\neg\metaA \rightarrow \always\neg\metaA &\quad \text{(P1 with $\neg\metaA$)} \\
  \neg\always\neg\metaA \rightarrow \neg\Box\neg\metaA &\quad \text{(contraposition)} \\
  \sometimes\metaA \rightarrow \Diamond\metaA &\quad \text{(abbreviation)}
\end{align*}

\vspace{0.1in}
\noindent The remaining perpetuity principles P3-P6 can be derived using similar techniques. For instance, P3 follows from P1 and MK (modal necessitation); P4 is the dual of P3; P5 requires MF, TF, and S5 properties; P6 combines P1 with modal reasoning. Complete derivations are left as exercises.

\section*{\it Problem Set: TM Syntax}

\begin{enumerate}[leftmargin=1.2in,itemsep=2pt]\small
  \item[\bf Well-Formedness:] Determine which of the following are well-formed sentences of $\BL$:
    \begin{enumerate}[label=\arabic*.]\small
      \begin{multicols}{2}
        \item $\Box p_1 \rightarrow \Future p_2$
        \item $\Past \Box p_1$
        \item $\always \sometimes p_1$
        \item $\Box \Future \Diamond p_1$
        \item $p_1 \Box p_2$
        \item $\Past \rightarrow p_1$
        \item $\always (\Box p_1 \leftrightarrow \Future p_2)$
        \item $\Diamond \past \Future p_1$
      \end{multicols}
    \end{enumerate}

  \item[\bf Abbreviations:] Expand the following abbreviated formulas into primitive $\BL$ notation (using only $\bot$, $\rightarrow$, $\Box$, $\Past$, $\Future$):
    \begin{enumerate}[label=\arabic*.]\small
      \begin{multicols}{2}
        \item $\always p_1$
        \item $\Diamond \sometimes p_1$
        \item $\past \metaA \vee \future \metaB$
        \item $\Box \metaA \wedge \always \metaB$
      \end{multicols}
    \end{enumerate}

  \item[\bf Complexity:] Define a complexity function $\comp{\metaA}$ that counts the number of operator symbols ($\Box$, $\Past$, $\Future$, $\neg$, $\rightarrow$, etc.) in a formula. Compute:
    \begin{enumerate}[label=\arabic*.]\small
      \begin{multicols}{2}
        \item $\comp{\always p_1}$
        \item $\comp{\Box \Future \Diamond p_1}$
        \item $\comp{\sometimes (\metaA \rightarrow \Box \metaB)}$
        \item $\comp{\Past \past p_1 \wedge \future \Future p_2}$
      \end{multicols}
    \end{enumerate}

  \item[\bf Duality:] Prove that the following pairs are duals (i.e., one is equivalent to the negation of the other):
    \begin{enumerate}[label=\arabic*.]\small
      \item $\Box \metaA$ and $\Diamond \metaA$
      \item $\Past \metaA$ and $\past \metaA$
      \item $\always \metaA$ and $\sometimes \metaA$
    \end{enumerate}

  \item[\bf Scope Ambiguity:] Provide fully parenthesized versions of the following to clarify intended scope:
    \begin{enumerate}[label=\arabic*.]\small
      \begin{multicols}{2}
        \item $\Box \metaA \rightarrow \Future \metaB \vee \metaC$
        \item $\always \metaA \wedge \Box \metaB \rightarrow \past \metaC$
      \end{multicols}
    \end{enumerate}
\end{enumerate}

\section*{\it Problem Set: TM Derivations}

\begin{enumerate}[leftmargin=1.2in,itemsep=2pt]\small
  \item[\bf Basic Derivations:] Provide Hilbert-style proofs of the following in the TM system:
    \begin{enumerate}[label=\arabic*.]\small
      \item $\TMproves \Box(\metaA \rightarrow \metaB) \rightarrow (\Box\metaA \rightarrow \Box\metaB)$ (modal K axiom)
      \item $\TMproves \Future(\metaA \rightarrow \metaB) \rightarrow (\Future\metaA \rightarrow \Future\metaB)$ (temporal K)
      \item $\TMproves \Box\metaA \rightarrow \Diamond\metaA$ (T implies D)
      \item $\TMproves \metaA \rightarrow \past\metaA$ (present implies past possibility)
    \end{enumerate}

  \item[\bf Perpetuity Theorems:] Prove the following perpetuity principles using the TM axioms:
    \begin{enumerate}[label=\arabic*.]\small
      \item[\bf P3:] $\TMproves \Box\metaA \rightarrow \Box\always\metaA$ \quad (\textit{Hint:} Use P1 and MK)
      \item[\bf P4:] $\TMproves \Diamond\sometimes\metaA \rightarrow \Diamond\metaA$ \quad (\textit{Hint:} Dualize P3)
      \item[\bf P9:] $\TMproves \neg\always\metaA \leftrightarrow \sometimes\neg\metaA$ \quad (\textit{Hint:} De Morgan's laws)
      \item[\bf P10:] $\TMproves \neg\sometimes\metaA \leftrightarrow \always\neg\metaA$ \quad (\textit{Hint:} De Morgan's laws)
    \end{enumerate}

  \item[\bf Interaction Principles:] Prove theorems involving both modal and temporal operators:
    \begin{enumerate}[label=\arabic*.]\small
      \item $\TMproves \Box\always\metaA \rightarrow \always\Box\metaA$ \quad (\textit{Hint:} Use MF, TF, and temporal reasoning)
      \item $\TMproves \Diamond\sometimes\metaA \rightarrow \sometimes\Diamond\metaA$ \quad (\textit{Hint:} Dualize previous)
      \item $\TMproves \Box\Future\metaA \rightarrow \Future\Box\metaA$ \quad (\textit{Hint:} Direct from MF and TF)
    \end{enumerate}

  \item[\bf Temporal Logic:] Prove properties specific to the temporal fragment:
    \begin{enumerate}[label=\arabic*.]\small
      \item $\TMproves \Past\metaA \rightarrow \Past\Past\metaA$ (past is transitive)
      \item $\TMproves \always\metaA \leftrightarrow \Past\metaA \wedge \Future\metaA$ (\textit{Hint:} Use abbreviation and propositional logic)
      \item $\TMproves \always(\metaA \wedge \metaB) \leftrightarrow (\always\metaA \wedge \always\metaB)$ (always distributes over conjunction)
    \end{enumerate}

  \item[\bf Modal Logic:] Prove that TM extends S5 modal logic:
    \begin{enumerate}[label=\arabic*.]\small
      \item $\TMproves \Diamond\Box\metaA \rightarrow \Box\metaA$ (S5 characteristic axiom)
      \item $\TMproves \Box(\Box\metaA \rightarrow \metaA)$ (necessitation of T)
      \item $\TMproves \Diamond\Diamond\metaA \leftrightarrow \Diamond\metaA$ (S4 for possibility)
    \end{enumerate}

  \item[\bf Derived Rules:] Show that the following derived rules are admissible in TM:
    \begin{enumerate}[label=\arabic*.]\small
      \item If $\TMproves \metaA \leftrightarrow \metaB$, then $\TMproves \Box\metaA \leftrightarrow \Box\metaB$ (modal replacement)
      \item If $\TMproves \metaA \leftrightarrow \metaB$, then $\TMproves \Future\metaA \leftrightarrow \Future\metaB$ (temporal replacement)
      \item If $\TMproves \metaA$, then $\TMproves \always\metaA$ (temporal necessitation)
    \end{enumerate}
\end{enumerate}

\section*{\sc TM Logic: Task Semantics}

The axiomatic system for TM logic gives us proof-theoretic means to derive theorems, but to understand what these formulas \textit{mean}, we need a semantic framework. Traditional modal logics use Kripke semantics, where formulas are evaluated at possible worlds in a frame. TM logic introduces a novel \textbf{task semantics}, where formulas are evaluated at \textit{world histories} rather than primitive possible worlds. A world history is a temporally connected sequence of world states, constrained by a parameterized task relation that encodes possible transitions over time.

\begin{enumerate}[leftmargin=1.2in]

  \item[\bf Task Frame:] A \textbf{task frame} is a triple $\taskframe = \tuple{\worldstateset, \Tor, \taskrel}$ where:
    \begin{itemize}
      \item $\worldstateset$ is a non-empty set of \textbf{world states}
      \item $\Tor = \tuple{T, +, \leq}$ is a \textbf{totally ordered abelian group} (a set $T$ of times equipped with an associative, commutative binary operation $+$ with identity element $0$, inverses for all elements, and a total order $\leq$ respecting the group operation)
      \item $\taskrel \subseteq \worldstateset \times T \times \worldstateset$ is a parameterized \textbf{task relation}
    \end{itemize}
    We write $w \taskrel_x v$ to mean $\tuple{w,x,v} \in \taskrel$, read as ``world state $v$ is accessible from world state $w$ via a task parameterized by time interval $x$''. The abelian group structure allows time-shift operations and interval arithmetic essential for semantic proofs.

  \item[\it Nullity Constraint:] For all $w \in \worldstateset$, we have $w \taskrel_0 w$. \\
    \textit{Interpretation:} Every world state can perform the null task (doing nothing) and remain at itself.

  \item[\it Compositionality Constraint:] For all $w, u, v \in \worldstateset$ and all $x, y \in T$, if $w \taskrel_x u$ and $u \taskrel_y v$, then $w \taskrel_{x+y} v$. \\
    \textit{Interpretation:} Tasks compose: if a task parameterized by time interval $x$ takes $w$ to $u$, and a task parameterized by interval $y$ takes $u$ to $v$, then the composite task parameterized by interval $x+y$ takes $w$ to $v$.

  \item[\bf World History:] A \textbf{world history} over a frame $\taskframe = \tuple{\worldstateset, \Tor, \taskrel}$ is a function $\worldhist: X \rightarrow \worldstateset$ where $X \subseteq T$ is a convex subset of times (i.e., if $x_1, x_2 \in X$ and $x_1 \leq z \leq x_2$, then $z \in X$) such that:
    \begin{itemize}
      \item \textbf{Task Coherence:} For all times $x, y \in T$ where both $x, x+y \in X$, we have $\worldhist(x) \taskrel_y \worldhist(x+y)$.
    \end{itemize}
    Intuitively, a world history is a temporally connected path through world states that respects the task relation. We write $H_{\taskframe}$ (or simply $\worldhistset$) for the set of all world histories over a given frame. The task coherence constraint ensures world states in a history are reachable via appropriate task transitions.

  \item[\bf Task Model:] A \textbf{task model} is a tuple $\taskmodel = \tuple{\worldstateset, \Tor, \taskrel, \corner{\cdot}}$ where $\tuple{\worldstateset, \Tor, \taskrel}$ is a task frame and $\corner{p_i} \subseteq \worldstateset$ is the \textbf{interpretation} of sentence letter $p_i$ (the set of world states at which $p_i$ is true). We write $w \in \corner{p_i}$ to mean $p_i$ is true at world state $w$.

  \item[\bf Truth Conditions:] Given a task model $\taskmodel$, world history $\worldhist$, and time $x$, we define truth at $\taskmodel, \worldhist, x$ (written $\taskmodel, \worldhist, x \models \metaA$) inductively:
    \begin{enumerate}[leftmargin=.5in,itemsep=.075in]
      \item $\taskmodel, \worldhist, x \models p_i$ \quad iff \quad $x \in \textit{dom}(\worldhist)$ and $\worldhist(x) \in \corner{p_i}$
      \item $\taskmodel, \worldhist, x \not\models \bot$ \quad (absurdity is always false)
      \item $\taskmodel, \worldhist, x \models \metaA \rightarrow \metaB$ \quad iff \quad $\taskmodel, \worldhist, x \not\models \metaA$ or $\taskmodel, \worldhist, x \models \metaB$
      \item $\taskmodel, \worldhist, x \models \Box \metaA$ \quad iff \quad $\taskmodel, \sigma, x \models \metaA$ for all $\sigma \in \worldhistset$
      \item $\taskmodel, \worldhist, x \models \Past \metaA$ \quad iff \quad $\taskmodel, \worldhist, y \models \metaA$ for all $y \in \textit{dom}(\worldhist)$ with $y < x$
      \item $\taskmodel, \worldhist, x \models \Future \metaA$ \quad iff \quad $\taskmodel, \worldhist, y \models \metaA$ for all $y \in \textit{dom}(\worldhist)$ with $y > x$
    \end{enumerate}
    \textit{Key insight:} Temporal operators ($\Past$, $\Future$) quantify over times within the \textit{same} world history $\worldhist$, while the modal operator ($\Box$) quantifies over \textit{all} world histories at the \textit{fixed} time $x$. This is what makes task semantics bimodal.

  \item[\bf Validity:] A formula $\metaA$ is \textbf{valid over task frames} (written $\models_{\textsc{tm}} \metaA$) iff for all task models $\taskmodel$, all world histories $\worldhist$, and all times $x \in \textit{dom}(\worldhist)$, we have $\taskmodel, \worldhist, x \models \metaA$.

  \item[\bf Logical Consequence:] A formula $\metaA$ is a \textbf{logical consequence} of a set $\Gamma$ of formulas (written $\Gamma \models_{\textsc{tm}} \metaA$) iff for all task models $\taskmodel$, all world histories $\worldhist$, and all times $x \in \textit{dom}(\worldhist)$, if $\taskmodel, \worldhist, x \models \gamma$ for all $\gamma \in \Gamma$, then $\taskmodel, \worldhist, x \models \metaA$.

\end{enumerate}

\vspace{10pt}
\noindent\textbf{Time-Shift Relations and Semantic Proofs}

To prove semantic validity of perpetuity principles, we need a key technical device: \textbf{time-shifted world histories}. The idea is that if a formula is true at a world history $\worldhist$ at time $y$, we can construct another world history $\sigma$ where the same formula is true at a different time $x$, by ``shifting'' the entire history through time. This is made possible by the abelian group structure of the temporal order.

\begin{enumerate}[leftmargin=1.2in]

  \item[\bf Time-Shift:] Given a frame $\taskframe = \tuple{\worldstateset, \Tor, \taskrel}$, world histories $\worldhist, \sigma \in \worldhistset$ are \textbf{time-shifted from $y$ to $x$} (written $\worldhist \approx_y^x \sigma$) if and only if there exists an \textbf{order automorphism} $\bar{a}: T \rightarrow T$ where $y = \bar{a}(x)$, $\textit{dom}(\sigma) = \bar{a}^{-1}(\textit{dom}(\worldhist))$, and $\sigma(z) = \worldhist(\bar{a}(z))$ for all $z \in \textit{dom}(\sigma)$.

  An order automorphism is a bijection that preserves the order relation: if $z_1 \leq z_2$, then $\bar{a}(z_1) \leq \bar{a}(z_2)$.

  \item[\bf Existence Theorem:] For any frame $\taskframe$, world history $\worldhist \in \worldhistset$, and times $x, y \in T$, there exists a world history $\sigma \in \worldhistset$ where $\worldhist \approx_y^x \sigma$ is witnessed by the automorphism $\bar{a}(z) = z - x + y$.

  \textit{Proof Sketch:} The function $\bar{a}(z) = z - x + y$ is a bijection (inverse: $\bar{a}^{-1}(z) = z + x - y$) that preserves order by the properties of addition in the abelian group $\Tor$. Define $\sigma(z) = \worldhist(\bar{a}(z))$ for all $z$ where $\bar{a}(z) \in \textit{dom}(\worldhist)$. Since $\textit{dom}(\worldhist)$ is convex, $\textit{dom}(\sigma) = \bar{a}^{-1}(\textit{dom}(\worldhist))$ is also convex. The task coherence property is preserved because $\sigma(z) \taskrel_d \sigma(z+d)$ iff $\worldhist(\bar{a}(z)) \taskrel_d \worldhist(\bar{a}(z)+d)$ iff $\worldhist(\bar{a}(z)) \taskrel_d \worldhist(\bar{a}(z+d))$, which holds since $\worldhist$ is a world history and $\bar{a}(z+d) = \bar{a}(z) + d$ by the group operation. $\square$

  \item[\bf Preservation Lemma:] For any model $\taskmodel$, formula $\metaA$, and world histories $\worldhist, \sigma \in \worldhistset$ where $\worldhist \approx_y^x \sigma$ is witnessed by $\bar{a}(z) = z - x + y$, we have $\taskmodel,\worldhist,y \models \metaA$ if and only if $\taskmodel,\sigma,x \models \metaA$.

  \textit{Proof Sketch:} By structural induction on formula complexity. Base cases (sentence letters, $\bot$) follow from the definition of time-shift: $\worldhist(y) = \worldhist(\bar{a}(x)) = \sigma(x)$. The implication case follows from the inductive hypothesis. For the modal case $\Box\metaA$, suppose $\taskmodel,\worldhist,y \not\models \Box\metaA$. Then there exists $\rho \in \worldhistset$ with $\taskmodel,\rho,y \not\models \metaA$. By the Existence Theorem, there exists $\rho' \in \worldhistset$ where $\rho \approx_y^x \rho'$. By inductive hypothesis, $\taskmodel,\rho',x \not\models \metaA$, so $\taskmodel,\sigma,x \not\models \Box\metaA$. The converse direction is symmetric. For temporal cases ($\Past$, $\Future$), use the fact that $\bar{a}$ is an order automorphism: $y' < y$ iff $\bar{a}^{-1}(y') < x$ because $\bar{a}$ preserves order. $\square$

\end{enumerate}

\vspace{10pt}
\noindent\textbf{Example 1: Simple Task Frame}

Consider a task frame with two world states: $\worldstateset = \set{w_0, w_1}$, linear time $\Tor = \tuple{\mathbb{R}, <}$, and task relation defined by:
\begin{itemize}
  \item $w_0 \taskrel_0 w_0$, $w_1 \taskrel_0 w_1$ (nullity)
  \item $w_0 \taskrel_1 w_1$ (one-unit transition from $w_0$ to $w_1$)
  \item $w_0 \taskrel_x w_0$ for all $x \geq 0$ (staying at $w_0$)
  \item $w_1 \taskrel_x w_1$ for all $x \geq 0$ (staying at $w_1$)
  \item $w_0 \taskrel_x w_1$ for all $x \geq 1$ (compositionality)
\end{itemize}

Let valuation $\corner{p} = \set{w_1}$ (so $p$ is true only at $w_1$). Consider world history $\worldhist: \mathbb{R} \rightarrow \worldstateset$ with $\worldhist(t) = w_0$ for $t \leq 0$ and $\worldhist(t) = w_1$ for $t > 0$. Then:
\begin{itemize}
  \item $\taskmodel, \worldhist, -1 \not\models p$ (since $\worldhist(-1) = w_0 \notin \corner{p}$)
  \item $\taskmodel, \worldhist, 1 \models p$ (since $\worldhist(1) = w_1 \in \corner{p}$)
  \item $\taskmodel, \worldhist, 0 \models \Future p$ (since $p$ true at all $t > 0$)
  \item $\taskmodel, \worldhist, 0 \models \Past \neg p$ (since $\neg p$ true at all $t < 0$)
\end{itemize}

\vspace{10pt}
\noindent\textbf{Theorem (Semantic Validity of Perpetuity Principles):} The formulas $\Box\metaA \rightarrow \always\metaA$ (P1) and $\sometimes\metaA \rightarrow \Diamond\metaA$ (P2) are valid over all task frames.

\textit{Proof:} We prove P1; P2 follows by contraposition and duality.

Suppose for contradiction there exists a task model $\taskmodel$, world history $\worldhist \in \worldhistset$, and time $x \in T$ such that $\taskmodel,\worldhist,x \models \Box\metaA$ but $\taskmodel,\worldhist,x \not\models \always\metaA$.

Since $\always\metaA \coloneq \Past\metaA \wedge \metaA \wedge \Future\metaA$, we have either $\taskmodel,\worldhist,x \not\models \Past\metaA$ or $\taskmodel,\worldhist,x \not\models \metaA$ or $\taskmodel,\worldhist,x \not\models \Future\metaA$.

From $\taskmodel,\worldhist,x \models \Box\metaA$, we know $\taskmodel,\sigma,x \models \metaA$ for all $\sigma \in \worldhistset$, so $\taskmodel,\worldhist,x \models \metaA$. Thus either $\taskmodel,\worldhist,x \not\models \Past\metaA$ or $\taskmodel,\worldhist,x \not\models \Future\metaA$, meaning there exists $y \in T$ where $\taskmodel,\worldhist,y \not\models \metaA$ and either $y < x$ or $y > x$.

\textit{Case 1} ($y < x$): By the Existence Theorem, there exists $\sigma \in \worldhistset$ where $\worldhist \approx_y^x \sigma$ witnessed by $\bar{a}(z) = z - x + y$. By the Preservation Lemma, $\taskmodel,\worldhist,y \models \metaA$ iff $\taskmodel,\sigma,x \models \metaA$. Since $\taskmodel,\worldhist,y \not\models \metaA$, we have $\taskmodel,\sigma,x \not\models \metaA$, contradicting $\taskmodel,\worldhist,x \models \Box\metaA$.

\textit{Case 2} ($y > x$): Symmetric to Case 1, using $\bar{a}(z) = z - x + y$ to shift from future time $y$ to present time $x$.

Therefore $\models_{\textsc{tm}} \Box\metaA \rightarrow \always\metaA$. By substituting $\neg\metaA$ for $\metaA$, we get $\models_{\textsc{tm}} \Box\neg\metaA \rightarrow \always\neg\metaA$, which by contraposition and the duality of operators gives $\models_{\textsc{tm}} \sometimes\metaA \rightarrow \Diamond\metaA$. $\square$

\vspace{10pt}
\noindent\textit{Key Insight:} This semantic proof demonstrates \textit{why} the perpetuity principles hold: the abelian group structure enables time-shifting any formula true at one time to another time, and the modal operator $\Box$ quantifies over all possible time-shifted world histories. Thus if $\metaA$ is necessary at $x$ (true in all world histories at $x$), it must also be true at all past and future times in the given history $\worldhist$, because those past/future times can be ``brought to'' time $x$ via time-shift automorphisms.

\section*{\it Problem Set: Task Semantics}

\begin{enumerate}[leftmargin=1.2in,itemsep=2pt]\small
  \item[\bf Frame Properties:] For each of the following, show that the described structure satisfies the nullity and compositionality constraints for a task frame:
    \begin{enumerate}[label=\arabic*.]\small
      \item $\worldstateset = \set{w}$ (single world state), $w \taskrel_x w$ for all $x \in \mathbb{R}$
      \item $\worldstateset = \mathbb{N}$ (natural numbers as states), $n \taskrel_x m$ iff $m \geq n$ and $x = m - n$
      \item $\worldstateset = \set{0, 1}$ (binary states), $i \taskrel_x j$ iff $x \geq 0$ and $i \leq j$
    \end{enumerate}

  \item[\bf World Histories:] Determine whether each of the following functions constitutes a valid world history in the task frame from Example 1:
    \begin{enumerate}[label=\arabic*.]\small
      \item $\worldhist_1: \mathbb{R} \rightarrow \set{w_0, w_1}$ with $\worldhist_1(t) = w_0$ for all $t$
      \item $\worldhist_2: \mathbb{R} \rightarrow \set{w_0, w_1}$ with $\worldhist_2(t) = w_1$ for all $t$
      \item $\worldhist_3: [0, 5] \rightarrow \set{w_0, w_1}$ with $\worldhist_3(t) = w_0$ for $t \in [0, 2)$ and $\worldhist_3(t) = w_1$ for $t \in [2, 5]$
      \item $\worldhist_4: [0, 5] \rightarrow \set{w_0, w_1}$ with $\worldhist_4(t) = w_1$ for $t \in [0, 2)$ and $\worldhist_4(t) = w_0$ for $t \in [2, 5]$
    \end{enumerate}

  \item[\bf Truth Evaluation:] Given the task model from Example 1 with valuation $\corner{p} = \set{w_1}$ and world history $\worldhist$ with $\worldhist(t) = w_0$ for $t \leq 0$ and $\worldhist(t) = w_1$ for $t > 0$, evaluate the following:
    \begin{enumerate}[label=\arabic*.]\small
      \item $\taskmodel, \worldhist, 0 \models p \vee \neg p$
      \item $\taskmodel, \worldhist, 0 \models \Past p$
      \item $\taskmodel, \worldhist, 0 \models \Future p$
      \item $\taskmodel, \worldhist, 0 \models \always p$
      \item $\taskmodel, \worldhist, 0 \models \sometimes p$
      \item $\taskmodel, \worldhist, 1 \models \Past \neg p$
    \end{enumerate}

  \item[\bf Modal Evaluation:] Consider a task frame with $\worldstateset = \set{w_0, w_1}$ and valuation $\corner{p} = \set{w_1}$. Let $\worldhist$ be a history with $\worldhist(0) = w_0$. Show that:
    \begin{enumerate}[label=\arabic*.]\small
      \item $\taskmodel, \worldhist, 0 \not\models \Box p$ (find a counterexample history)
      \item $\taskmodel, \worldhist, 0 \models \Diamond p$ (demonstrate the existential claim)
      \item If every history passing through time $0$ has $\sigma(0) = w_0$, then $\taskmodel, \worldhist, 0 \models \Box \neg p$
    \end{enumerate}

  \item[\bf Validity:] For each formula, determine whether it is valid over all task frames. If valid, sketch a semantic proof. If invalid, construct a countermodel.
    \begin{enumerate}[label=\arabic*.]\small
      \item $\Box \metaA \rightarrow \metaA$ (T axiom)
      \item $\Past \metaA \rightarrow \metaA$ (does past truth imply present truth?)
      \item $\always \metaA \rightarrow \Box \metaA$ (does temporal necessity imply modal necessity?)
      \item $\Box \metaA \rightarrow \always \metaA$ (perpetuity principle P1)
    \end{enumerate}

  \item[\bf Countermodels:] Construct explicit countermodels showing the following are \textit{not} valid:
    \begin{enumerate}[label=\arabic*.]\small
      \item $\Diamond \metaA \rightarrow \always \metaA$ (possibility doesn't imply temporal necessity)
      \item $\sometimes \metaA \rightarrow \Box \metaA$ (temporal possibility doesn't imply modal necessity)
      \item $\Past \metaA \rightarrow \Future \metaA$ (past truth doesn't imply future truth)
    \end{enumerate}

  \item[\bf Time-Shift Understanding:] Consider the time-shift relation $\worldhist \approx_y^x \sigma$ with automorphism $\bar{a}(z) = z - x + y$:
    \begin{enumerate}[label=\arabic*.]\small
      \item Given $\worldhist: \mathbb{R} \rightarrow \worldstateset$ and $x = 0$, $y = 5$, compute $\bar{a}(z)$ explicitly and find $\textit{dom}(\sigma)$ in terms of $\textit{dom}(\worldhist)$.
      \item Show that if $\worldhist(-3) = w_0$, then $\sigma(2) = w_0$ (using the automorphism from part 1).
      \item Explain why the abelian group structure of $\Tor$ is essential for time-shift operations (what would fail if $T$ lacked inverse elements?).
    \end{enumerate}

  \item[\bf Applying the Preservation Lemma:] Given world histories $\worldhist, \sigma \in \worldhistset$ where $\worldhist \approx_{-2}^{3} \sigma$:
    \begin{enumerate}[label=\arabic*.]\small
      \item If $\taskmodel,\worldhist,-2 \models p \wedge \Future q$, what can you conclude about $\taskmodel,\sigma,3$?
      \item If $\taskmodel,\sigma,3 \models \Box r$, what can you conclude about $\taskmodel,\worldhist,-2$?
      \item Why doesn't the Preservation Lemma tell us anything about $\taskmodel,\worldhist,5$ from facts about $\sigma$?
    \end{enumerate}

  \item[\bf Task Relation Design:] Design a task frame and valuation that validates the following formula at some world history and time:
    \begin{enumerate}[label=\arabic*.]\small
      \item $\Diamond p \wedge \Diamond \neg p$ (both $p$ and $\neg p$ are modally possible)
      \item $\sometimes p \wedge \always \neg p$ (impossible: $p$ sometimes and always not)
      \item $\Box \Future p \wedge \neg p$ (necessarily will be $p$, but not now)
    \end{enumerate}

\end{enumerate}

\section*{\sc TM Logic: Metatheory}

We have now defined both the proof-theoretic (axiomatic) and semantic (task frames) aspects of TM logic. The axiomatic system (TM Logic: Axiomatic System) showed us \textit{how} to derive theorems syntactically, while the task semantics (TM Logic: Task Semantics) showed us \textit{what} formulas mean and \textit{why} perpetuity principles hold. The semantic validity proof using time-shift automorphisms demonstrated that P1 and P2 are true in virtue of the mathematical structure of task frames---specifically, the abelian group structure enabling time translations.

The central metalogical questions are: (1) \textbf{Soundness} -- does every derivable formula hold in all task models? and (2) \textbf{Completeness} -- does every semantically valid formula have a proof? These questions establish the correspondence between syntactic derivability and semantic validity.

\begin{enumerate}[leftmargin=1.2in]

  \item[\bf Soundness Theorem:] \textit{If $\Gamma \TMproves \metaA$, then $\Gamma \models_{\textsc{tm}} \metaA$.}

  That is, every TM-derivable formula is semantically valid over all task frames. This means the proof system is \textbf{reliable}: if we can prove something syntactically, it is guaranteed to be true semantically.

  \item[\bf Soundness Proof Sketch:] The proof proceeds by induction on the length of TM-proofs. We show: (1) all axiom schemata are valid over task frames, and (2) all inference rules preserve validity. We demonstrate representative cases:

  \begin{enumerate}[leftmargin=.7in,itemsep=.15in]
    \item[\it Case MP:] If $\metaA$ and $\metaA \rightarrow \metaB$ are both valid, then $\metaB$ is valid.

    \textit{Proof:} Suppose $\taskmodel, \worldhist, x \models \metaA$ and $\taskmodel, \worldhist, x \models \metaA \rightarrow \metaB$ for arbitrary model, history, and time. By truth condition for $\rightarrow$, either $\taskmodel, \worldhist, x \not\models \metaA$ or $\taskmodel, \worldhist, x \models \metaB$. Since we have $\taskmodel, \worldhist, x \models \metaA$, it must be that $\taskmodel, \worldhist, x \models \metaB$. Thus $\metaB$ is valid. \qed

    \item[\it Case MK:] The axiom $\Box(\metaA \rightarrow \metaB) \rightarrow (\Box\metaA \rightarrow \Box\metaB)$ is valid.

    \textit{Proof:} Suppose $\taskmodel, \worldhist, x \models \Box(\metaA \rightarrow \metaB)$. This means for all histories $\sigma$ with $x \in \textit{dom}(\sigma)$, we have $\taskmodel, \sigma, x \models \metaA \rightarrow \metaB$. Now suppose $\taskmodel, \worldhist, x \models \Box\metaA$, meaning for all such histories $\sigma$, we have $\taskmodel, \sigma, x \models \metaA$. Take any history $\sigma$ with $x \in \textit{dom}(\sigma)$. We have both $\taskmodel, \sigma, x \models \metaA$ and $\taskmodel, \sigma, x \models \metaA \rightarrow \metaB$, so by truth condition for $\rightarrow$, we get $\taskmodel, \sigma, x \models \metaB$. Since $\sigma$ was arbitrary, $\taskmodel, \worldhist, x \models \Box\metaB$. \qed

    \item[\it Case TK:] The axiom $\Future(\metaA \rightarrow \metaB) \rightarrow (\Future\metaA \rightarrow \Future\metaB)$ is valid.

    \textit{Proof:} Similar to MK, but quantifying over times $y > x$ within the same history $\worldhist$ rather than over all histories at time $x$. \qed

    \item[\it Case MF:] The bimodal axiom $\Box\Future\metaA \rightarrow \Future\Box\metaA$ is valid.

    \textit{Proof:} Suppose $\taskmodel, \worldhist, x \models \Box\Future\metaA$. This means for all histories $\sigma$ with $x \in \textit{dom}(\sigma)$, we have $\taskmodel, \sigma, x \models \Future\metaA$, i.e., for all $y > x$ in $\textit{dom}(\sigma)$, we have $\taskmodel, \sigma, y \models \metaA$.

    To show $\taskmodel, \worldhist, x \models \Future\Box\metaA$, take any $y > x$ in $\textit{dom}(\worldhist)$. We need to show $\taskmodel, \worldhist, y \models \Box\metaA$. Take any history $\rho$ with $y \in \textit{dom}(\rho)$. By assumption applied to $\rho$, we have $\taskmodel, \rho, y \models \metaA$. Since $\rho$ was arbitrary, $\taskmodel, \worldhist, y \models \Box\metaA$. Since $y$ was arbitrary, $\taskmodel, \worldhist, x \models \Future\Box\metaA$. \qed

    \item[\it Case TF:] The bimodal axiom $\Box\metaA \rightarrow \Box\Future\metaA$ is valid.

    \textit{Proof:} Suppose $\taskmodel, \worldhist, x \models \Box\metaA$. To show $\taskmodel, \worldhist, x \models \Box\Future\metaA$, take any history $\sigma$ with $x \in \textit{dom}(\sigma)$. We need $\taskmodel, \sigma, x \models \Future\metaA$. Take any $y > x$ in $\textit{dom}(\sigma)$. By assumption, $\taskmodel, \sigma, y \models \metaA$ (since necessity holds at all histories, including $\sigma$, at all times). Thus $\taskmodel, \sigma, x \models \Future\metaA$, and since $\sigma$ was arbitrary, $\taskmodel, \worldhist, x \models \Box\Future\metaA$. \qed
  \end{enumerate}

  The remaining axioms (MT, M4, MB, TD, TL, T4, TA) and rules (TK, TD, MK) are shown valid by similar arguments. The key insight is that the bimodal axioms MF and TF ensure that modal and temporal operators interact correctly across world histories and times.

  \item[\bf Completeness Theorem:] \textit{If $\Gamma \models_{\textsc{tm}} \metaA$, then $\Gamma \TMproves \metaA$.}

  That is, every semantically valid formula is TM-derivable. This means the proof system is \textbf{adequate}: if something is true in all task models, we can find a proof of it.

  \item[\bf Completeness Proof Strategy:] The proof uses the \textbf{canonical model construction} method standard in modal logic. The idea is to build a task model from maximal consistent sets of formulas, where:
  \begin{itemize}
    \item World states are maximal consistent sets (sets of formulas closed under consequence and not containing contradictions)
    \item The task relation $w \taskrel_x v$ is defined by formula consistency: if $\Future^x \metaA \in w$, then $\metaA \in v$
    \item Histories are constructed as maximal linear extensions of consistent temporal sequences
    \item The valuation assigns truth to sentence letters at worlds where they appear
  \end{itemize}

  The key lemma is the \textbf{Truth Lemma}: for any formula $\metaA$ and maximal consistent set $w$ representing a world state in the canonical model, $\metaA \in w$ iff the canonical model validates $\metaA$ at $w$. This is proved by induction on formula complexity, with the modal and temporal cases following from the construction of the task relation and histories.

  Given the Truth Lemma, completeness follows: if $\Gamma \models_{\textsc{tm}} \metaA$ but $\Gamma \not\TMproves \metaA$, then $\Gamma \cup \set{\neg\metaA}$ is consistent (by negation completeness). This set can be extended to a maximal consistent set, which forms a world in the canonical model where $\Gamma$ holds but $\metaA$ fails -- contradicting semantic validity.

  \item[\bf Omitted Details:] The full completeness proof is technically demanding, requiring:
  \begin{itemize}
    \item Lindenbaum's Lemma for extending consistent sets to maximal consistent sets
    \item Careful construction of the temporal order from modal-temporal formulas
    \item Verification that the canonical task relation satisfies nullity and compositionality
    \item Proof that canonical world histories satisfy task consistency
    \item Detailed induction for the Truth Lemma, handling quantification over all histories
  \end{itemize}
  For a complete treatment, see the original completeness proof in the source literature. The result establishes that TM logic has both a \textbf{sound and complete axiomatization} with respect to task semantics.

  \item[\bf Metalogical Consequences:] The soundness and completeness theorems have several important consequences:
  \begin{itemize}
    \item \textbf{Consistency:} TM is consistent (no formula is both provable and refutable) since the task semantics provides models
    \item \textbf{Decidability:} TM is decidable (there exists an algorithm to determine provability) inherited from decidability of S5 modal logic and tense logic
    \item \textbf{Compactness:} If every finite subset of $\Gamma$ is satisfiable, then $\Gamma$ is satisfiable (standard consequence of completeness)
    \item \textbf{Expressiveness:} TM can express perpetuity principles (like P1-P6) that are not derivable in either pure modal logic or pure tense logic alone
  \end{itemize}

\end{enumerate}

\section*{\it Problem Set: TM Metatheory}

\begin{enumerate}[leftmargin=1.2in,itemsep=2pt]\small
  \item[\bf Axiom Validity:] Show that the following TM axioms are valid over all task frames by providing explicit semantic proofs:
    \begin{enumerate}[label=\arabic*.]\small
      \item MT: $\Box\metaA \rightarrow \metaA$ (\textit{Hint:} Use reflexivity of task relation via nullity)
      \item TD: $\metaA \rightarrow \Past\metaA$ (\textit{Hint:} Consider the universal quantifier over empty past)
      \item T4: $\Past\metaA \rightarrow \Past\Past\metaA$ (\textit{Hint:} Transitivity of temporal order)
    \end{enumerate}

  \item[\bf Rule Soundness:] Prove that the following inference rules preserve semantic validity:
    \begin{enumerate}[label=\arabic*.]\small
      \item MK: If $\TMproves \metaA$, then $\TMproves \Box\metaA$ (modal necessitation preserves validity)
      \item TK: If $\TMproves \metaA$, then $\TMproves \Future\metaA$ (temporal necessitation preserves validity)
      \item TD: If $\TMproves \metaA$, then $\TMproves \Past\metaA$ (past necessitation preserves validity)
    \end{enumerate}

  \item[\bf Consistency:] For each of the following sets of formulas, determine whether it is consistent (satisfiable in some task model) or inconsistent. If consistent, describe a task model that satisfies it. If inconsistent, derive a contradiction using TM axioms.
    \begin{enumerate}[label=\arabic*.]\small
      \item $\set{\Box p, \Diamond \neg p}$
      \item $\set{\always p, \sometimes \neg p}$
      \item $\set{\Box p, \neg\always p}$
      \item $\set{\sometimes p, \always \neg p}$
    \end{enumerate}

  \item[\bf Countermodels:] For each invalid formula, use the completeness theorem to argue that a countermodel must exist, then construct an explicit task model refuting it:
    \begin{enumerate}[label=\arabic*.]\small
      \item $\always \metaA \rightarrow \Box \metaA$ (temporal necessity doesn't imply modal necessity)
      \item $\Diamond \metaA \rightarrow \sometimes \metaA$ (modal possibility doesn't imply temporal possibility)
      \item $\Past \metaA \wedge \Future \metaA \rightarrow \metaA$ (past and future don't imply present)
    \end{enumerate}

  \item[\bf Decidability:] Given that TM is decidable:
    \begin{enumerate}[label=\arabic*.]\small
      \item Explain why decidability is useful for automated reasoning systems
      \item Describe how you would algorithmically check whether $\Box\always p \rightarrow \always\Box p$ is TM-valid
      \item Discuss why adding second-order quantification would likely destroy decidability
    \end{enumerate}

\end{enumerate}

\section*{\sc TM Logic: Extensions}

The TM logic we have developed so far makes minimal assumptions about the temporal order structure -- it is dense, unbounded, and linear. However, many applications require different temporal properties. We can extend TM by adding axioms that correspond to frame constraints on the temporal order. We also introduce additional temporal operators that are useful for reasoning about specific temporal structures.

\begin{enumerate}[leftmargin=1.2in]

  \item[\bf Frame Constraints:] A \textbf{frame constraint} is a condition on the temporal order $\Tor = \tuple{T, <}$ that corresponds to a validating axiom. Several important constraints:

  \begin{enumerate}[leftmargin=.7in,itemsep=.15in]
    \item[\it Dense Time:] The temporal order is \textbf{dense} if for all $x, y \in T$ with $x < y$, there exists $z \in T$ such that $x < z < y$. This corresponds to axiom \textbf{DN} (density): $\Future\metaA \rightarrow \Future\Future\metaA$. Intuitively, between any two times, there is always another time.

    \item[\it Discrete Time:] The temporal order is \textbf{discrete} if every time (except possibly a first or last time) has an immediate successor and predecessor. This corresponds to axioms involving the \textbf{Next} and \textbf{Previous} operators (defined below). Intuitively, time proceeds in discrete steps.

    \item[\it Bounded Time:] The temporal order is \textbf{bounded} if it has a first time (past boundary) or last time (future boundary). This corresponds to axioms like $\metaA \rightarrow \Past\top$ (there is always a past) or $\metaA \rightarrow \Future\top$ (there is always a future).

    \item[\it Linear Time:] The temporal order is \textbf{linear} if for all times $x, y$, either $x < y$, $x = y$, or $y < x$. This is already assumed in our base TM logic. Alternative logics allow \textbf{branching time} where multiple futures are possible.
  \end{enumerate}

  \item[\bf TM$^\textsc{d}$ System:] The system \textbf{TM$^\textsc{d}$} extends TM with the density axiom:
  \begin{itemize}
    \item \textbf{DN} (Density): $\Future\metaA \rightarrow \Future\Future\metaA$
  \end{itemize}
  In TM$^\textsc{d}$, we can prove additional theorems not derivable in base TM. For example:
  \begin{itemize}
    \item $\TMD \Future\metaA \leftrightarrow \Future\Future\metaA$ (future and future-future are equivalent under density)
    \item $\TMD \Past\metaA \leftrightarrow \Past\Past\metaA$ (past and past-past are equivalent under density)
  \end{itemize}
  These express that under dense time, "eventually" and "eventually eventually" mean the same thing.

  \item[\bf Additional Temporal Operators:] Beyond $\Past$ and $\Future$, several temporal operators are useful for specific reasoning tasks:

  \begin{enumerate}[leftmargin=.7in,itemsep=.15in]
    \item[\bf Next:] $\textbf{Next}\,\metaA$ means "$\metaA$ holds at the immediate next time" (defined in discrete time). In continuous dense time, \textbf{Next} can be defined as: \\
    $\textbf{Next}\,\metaA := \neg\Future\neg(\metaA \vee \Future\bot)$

    \item[\bf Previous:] $\textbf{Previous}\,\metaA$ means "$\metaA$ held at the immediate previous time" (defined in discrete time). In continuous dense time, \textbf{Previous} can be defined as: \\
    $\textbf{Previous}\,\metaA := \neg\Past\neg(\metaA \vee \Past\bot)$

    \item[\bf Since:] $\metaA\,\textbf{Since}\,\metaB$ means "$\metaA$ has held continuously since the last time $\metaB$ held". Formally: \\
    $\metaA\,\textbf{Since}\,\metaB := \Past(\metaB \wedge \Future\metaA)$

    \item[\bf Until:] $\metaA\,\textbf{Until}\,\metaB$ means "$\metaA$ will hold continuously until $\metaB$ holds". Formally: \\
    $\metaA\,\textbf{Until}\,\metaB := \Future(\metaB \wedge \Past\metaA)$
  \end{enumerate}

  These operators are particularly useful in temporal logic for expressing complex temporal relationships in natural language and formal specifications.

  \item[\bf Frame Correspondence:] The relationship between axioms and frame properties is called \textbf{frame correspondence}. For TM logic:

  \begin{itemize}
    \item $\Future\metaA \rightarrow \Future\Future\metaA$ corresponds to density of $<$
    \item $\metaA \rightarrow \Past\metaA$ corresponds to unbounded past (no first time)
    \item $\metaA \rightarrow \Future\metaA$ would correspond to unbounded future (no last time)
    \item $\Box\metaA \rightarrow \metaA$ (MT axiom) corresponds to reflexivity of task relation (via nullity)
  \end{itemize}

  Frame correspondence allows us to reason about temporal structures by reasoning about formulas. If we want to restrict to models with dense time, we simply add the DN axiom to our proof system.

  \item[\bf Relationship to S5 Modal Logic:] Recall that the modal fragment of TM (ignoring temporal operators) is exactly S5 modal logic. The axioms M4 and MB ensure that the modal accessibility relation (viewing world histories at a fixed time as worlds) is an equivalence relation. This means:
  \begin{itemize}
    \item \textbf{Reflexivity} (from MT): $\Box\metaA \rightarrow \metaA$
    \item \textbf{Transitivity} (from M4): $\Box\metaA \rightarrow \Box\Box\metaA$
    \item \textbf{Symmetry} (from MB): $\metaA \rightarrow \Box\Diamond\metaA$
  \end{itemize}
  These axioms characterize S5, the strongest normal modal logic, where necessity is "logical necessity" rather than "physical possibility" or "epistemic possibility" (which correspond to weaker systems like S4 or T).

\end{enumerate}

\section*{\it Problem Set: TM Extensions}

\begin{enumerate}[leftmargin=1.2in,itemsep=2pt]\small
  \item[\bf Density Theorems:] Prove the following in the TM$^\textsc{d}$ system (TM + DN axiom):
    \begin{enumerate}[label=\arabic*.]\small
      \item $\TMD \Past\metaA \rightarrow \Past\Past\metaA$ (use DN with duality)
      \item $\TMD \Future\metaA \leftrightarrow \Future\Future\metaA$ (use DN both directions)
      \item $\TMD \sometimes\metaA \leftrightarrow (\past\metaA \vee \metaA \vee \future\metaA)$ (density collapses distinctions)
    \end{enumerate}

  \item[\bf Discrete Time:] Consider a task frame with discrete time (each time has immediate successor/predecessor). Define:
    \begin{itemize}
      \item $\textbf{Next}\,\metaA :=$ "$\metaA$ holds at the next time point"
      \item $\textbf{Previous}\,\metaA :=$ "$\metaA$ held at the previous time point"
    \end{itemize}
    Show that in discrete time:
    \begin{enumerate}[label=\arabic*.]\small
      \item $\Future\metaA \leftrightarrow \textbf{Next}\,\metaA \vee \textbf{Next}\,\Future\metaA$ (future means next or later)
      \item $\textbf{Next}\,\metaA \wedge \textbf{Next}\,\metaB \leftrightarrow \textbf{Next}\,(\metaA \wedge \metaB)$ (Next distributes over conjunction)
      \item $\textbf{Next}\,\bot$ is unsatisfiable (there must be a next time)
    \end{enumerate}

  \item[\bf Since and Until:] Using the definitions $\metaA\,\textbf{Since}\,\metaB := \Past(\metaB \wedge \Future\metaA)$ and $\metaA\,\textbf{Until}\,\metaB := \Future(\metaB \wedge \Past\metaA)$:
    \begin{enumerate}[label=\arabic*.]\small
      \item Prove: $\TMproves (\metaA\,\textbf{Since}\,\metaB) \rightarrow \Past\metaB$ (if since, then $\metaB$ was true)
      \item Prove: $\TMproves (\metaA\,\textbf{Until}\,\metaB) \rightarrow \Future\metaB$ (if until, then $\metaB$ will be true)
      \item Show by countermodel: $\not\models (\metaA\,\textbf{Since}\,\metaB) \leftrightarrow (\metaB\,\textbf{Until}\,\metaA)$ (not symmetric)
    \end{enumerate}

  \item[\bf Frame Correspondence:] For each axiom, determine which frame property it corresponds to:
    \begin{enumerate}[label=\arabic*.]\small
      \item $\metaA \rightarrow \Past\metaA$ corresponds to: (a) dense time, (b) unbounded past, (c) discrete time
      \item $\Past\metaA \rightarrow \Past\Past\metaA$ corresponds to: (a) transitivity of $<$, (b) density, (c) linearity
      \item $\Future\metaA \rightarrow \Future\Future\metaA$ corresponds to: (a) density of $<$, (b) reflexivity, (c) symmetry
    \end{enumerate}

  \item[\bf S5 Modal Fragment:] Show that TM extends S5 modal logic by proving:
    \begin{enumerate}[label=\arabic*.]\small
      \item $\TMproves \Box(\metaA \vee \metaB) \leftrightarrow (\Box\metaA \vee \Box\metaB)$ (characteristic S5 property)
      \item $\TMproves \Diamond\Box\metaA \leftrightarrow \Box\metaA$ (modal collapse in S5)
      \item $\TMproves \Box\Diamond\Box\metaA \leftrightarrow \Box\metaA$ (iterations collapse in S5)
    \end{enumerate}

  \item[\bf Integration Exercise:] Construct a task model satisfying all of the following simultaneously:
    \begin{enumerate}[label=\arabic*.]\small
      \item Dense temporal order (for every two times, another time exists between them)
      \item At least two distinct world states
      \item A sentence letter $p$ such that $\Box p$ is false but $\always p$ is true at some history and time
      \item Verify your model satisfies task frame constraints (nullity, compositionality)
    \end{enumerate}

\end{enumerate}


